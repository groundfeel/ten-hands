{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "ESLint",
      "type": "shell",
      "problemMatcher": "$eslint-stylish",
      "command": "npm run lint",
      "windows": {
        "command": "npm run lint"
      }
    },
    {
      "label": "ESLint:fix",
      "type": "shell",
      "problemMatcher": "$eslint-stylish",
      "command": "npm run lint:fix",
      "windows": {
        "command": "npm run lint:fix"
      }
    }
  ]
}
